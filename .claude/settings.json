{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "temporal-cli-mcp": {
      "command": "uvx",
      "args": ["git+https://github.com/eantyshev/temporal-cli-mcp", "temporal-cli-mcp"]
    }
  }
}
